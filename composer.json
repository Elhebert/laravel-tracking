{
    "name": "elhebert/laravel-tracking",
    "description": "A package to help you manage your tracking (and analytics) tools",
    "keywords": [
        "Laravel",
        "cookie",
        "GDPR",
        "tracking"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dieter Stinglhamber",
            "email": "elhebert@hotmail.be"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "elhebert/laravel-croustillon": "^0.0.4",
        "illuminate/support": "5.7.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "Elhebert\\Tracking\\": "src/"
        }
    },
    "scripts": {
        "cs-fix": [
            "./vendor/bin/php-cs-fixer fix --allow-risky yes"
        ]
    },
    "extra": {
        "laravel": {
        }
    }
}
